{
    "name": "whitedigital-eu/config-pack",
    "description": "PhpCsFixer configuration to use in other organization repositories",
    "type": "library",
    "keywords": [
        "config",
        "cs-fixer",
        "php",
        "php-cs-fixer",
        "phpstan",
        "symfony"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Davis Zalitis",
            "email": "davis.zalitis@whitedigital.eu",
            "homepage": "https://me.k0d3r1s.com"
        }
    ],
    "require": {
        "php": ">=8.2.0",
        "ext-random": "*",
        "api-platform/core": "*",
        "composer-runtime-api": ">=2.0.0",
        "doctrine/data-fixtures": "*",
        "doctrine/doctrine-fixtures-bundle": "*",
        "doctrine/persistence": "*",
        "fakerphp/faker": "*",
        "friendsofphp/php-cs-fixer": "*",
        "phpstan/phpstan": "*",
        "phpstan/phpstan-doctrine": "*",
        "phpstan/phpstan-symfony": "*",
        "phpunit/phpunit": ">=10.0",
        "roave/security-advisories": "dev-latest",
        "symfony/config": "*",
        "symfony/dependency-injection": "*",
        "symfony/framework-bundle": "*",
        "symfony/http-client": "*",
        "symfony/http-foundation": "*",
        "symfony/http-kernel": "*",
        "whitedigital-eu/entity-resource-mapper-bundle": "*",
        "whitedigital-eu/storage-item-resource": "*"
    },
    "suggest": {
        "whitedigital-eu/site-tree": "*"
    },
    "autoload": {
        "psr-4": {
            "WhiteDigital\\Config\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-main": "2.3.x-dev"
        },
        "thanks": {
            "name": "whitedigital-eu/config-pack",
            "url": "https://github.com/whitedigital-eu/config-pack"
        }
    }
}
