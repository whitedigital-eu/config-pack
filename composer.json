{
    "name": "whitedigital-eu/config-pack",
    "description": "PhpCsFixer configuration to use in other organization repositories",
    "type": "library",
    "keywords": [
        "config",
        "cs-fixer",
        "php",
        "php-cs-fixer",
        "phpstan",
        "symfony"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Davis Zalitis",
            "email": "davis.zalitis@whitedigital.eu",
            "homepage": "https://me.k0d3r1s.com"
        }
    ],
    "require": {
        "php": ">=8.1.0",
        "friendsofphp/php-cs-fixer": "*",
        "phpstan/phpstan": "*",
        "phpstan/phpstan-doctrine": "*",
        "phpstan/phpstan-symfony": "*",
        "phpunit/phpunit": ">=10.0",
        "roave/security-advisories": "dev-latest"
    },
    "autoload": {
        "psr-4": {
            "WhiteDigital\\Config\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-main": "2.x-dev"
        },
        "thanks": {
            "name": "whitedigital-eu/config-pack",
            "url": "https://github.com/whitedigital-eu/config-pack"
        }
    }
}
